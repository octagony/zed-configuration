{
	"tab_size": 4,
	"ui_font_size": 20,
	"ui_font_family": "JetBrainsMono Nerd Font",
	"buffer_font_size": 20,
	"buffer_font_family": "JetBrainsMono Nerd Font",
	"buffer_line_height": {
		"custom": 1.6
	},
	"buffer_font_features": {
		"calt": false
	},
	"theme": {
		"mode": "system",
		"light": "Catppuccin Mocha",
		"dark": "Catppuccin Mocha"
	},
	"bottom_dock_layout": "right_aligned",
	"auto_install_extensions": {
		"html": true,
		"scss": true,
		"vue": true,
		"toml": true,
		"svelte": true,
		"lua": true,
		"astro": true,
		"emmet": true
	},
	"autosave": "off",
	"restore_on_startup": "last_session",
	"current_line_highlight": "none",
	"tab_bar": {
		"show_nav_history_buttons": false
	},
	"tabs": {
		"file_icons": true,
		"git_status": false,
		"show_close_button": "always",
		"show_diagnostics": "all"
	},
	"toolbar": {
		"breadcrumbs": false,
		"quick_actions": false
	},
	"file_finder": {
		"modal_max_width": "medium"
	},
	"use_smartcase_search": true,
	"icon_theme": {
		"mode": "dark",
		"dark": "Catppuccin Mocha",
		"light": "Catppuccin Mocha"
	},
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},
	"hard_tabs": true,
	"use_system_path_prompts": false,
	"terminal": {
		"scrollbar": {
			"show": "never"
		}
	},
	"project_panel": {
		"default_width": 375,
		"dock": "right",
		"entry_spacing": "comfortable",
		"folder_icons": true,
		"git_status": true,
		"indent_size": 12,
		"scrollbar": {
			"show": "never"
		}
	},
	"git": {
		"inline_blame": {
			"enabled": false
		}
	},
	"format_on_save": "off"
}
